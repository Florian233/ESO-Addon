<GuiXml>
    <Controls>
        <TopLevelControl name="SetLockerControl" hidden="true" mouseEnabled="true" movable="true" clampedToScreen="true">
            <Anchor point="BOTTOM" relativeTo="GuiRoot" relativePoint="BOTTOM" offsetX="0" offsetY="-128"/>
            <Dimensions x="512" y="584" />
            <Controls>
                <Backdrop name="$(parent)BG" inherits="ZO_ThinBackdrop" mouseEnabled="true" movable="true" />
                <Label name="$(parent)Heading" font="ZoFontWindowTitle" color="CFDCBD" wrapMode="ELLIPSIS" verticalAlignment="CENTER" text="SetLocker">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="16" offsetY="10"/>
                </Label>
				<Button name="$(parent)_hide" inherits="ZO_ButtonBehaviorClickSound" >
                    <OnClicked>SetLocker.Close()</OnClicked>
                    <Dimensions x="40" y="40" />                                
                    <Anchor point="TOPRIGHT" relativeTo="$(parent)" relativePoint="TOPRIGHT" offsetX="-10" offsetY="10"  />
                    <Textures
                        normal    ="/esoui/art/buttons/decline_up.dds"
                        pressed    ="/esoui/art/buttons/decline_down.dds"
                        mouseOver  ="/esoui/art/buttons/decline_over.dds"
                     />
                </Button>
				<Button name="$(parent)_reset" inherits="ZO_ButtonBehaviorClickSound" >
                    <OnClicked>SetLocker.OpenResetQ()</OnClicked>
                    <Dimensions x="50" y="50" />                                
                    <Anchor point="TOPRIGHT" relativeTo="$(parent)" relativePoint="TOPRIGHT" offsetX="-50" offsetY="5"  />
                    <Textures
						normal		="esoui/art/help/help_tabicon_feedback_up.dds"
						pressed		="esoui/art/help/help_tabicon_feedback_down.dds "
						mouseOver	="esoui/art/help/help_tabicon_feedback_over.dds"
                     />
                </Button>
				<Button name="$(parent)_ShowLoot_Button" inherits="ZO_ButtonBehaviorClickSound" >
					<OnClicked>SetLocker.ShowLoot()</OnClicked>
                    <Dimensions x="35" y="35" />                                
                    <Anchor point="TOPLEFT" relativeTo="$(parent)Heading" relativePoint="BOTTOMLEFT" offsetX="0" offsetY="20"  />
                    <Textures 
						normal="esoui/art/cadwell/checkboxicon_unchecked.dds"
					/>  
                </Button>
				<Label name="$(parent)ShowLoot" font="ZoFontGameMedium" text="">
					<Anchor point="LEFT" relativeTo="$(parent)_ShowLoot_Button" relativePoint="LEFT" offsetX="37" offsetY="0" />
				</Label>
				<EditBox name="$(parent)_Text" multiLine="false" newLineEnabled="false" maxInputCharacters="50" inherits="ZO_DefaultEditForDarkBackdrop" text="Filter for sets">
					<Dimensions x="400" y="25" />
					<Anchor point="TOPLEFT" relativeTo="$(parent)ShowLoot" relativePoint="BOTTOMLEFT" offsetX="16" offsetY="30" />
					<Controls>
						<Backdrop name="$(parent)TextBG" centerColor="000000" edgeColor="AAAAAA">
							<AnchorFill />
							<Edge edgeSize="2" />
						</Backdrop>
					</Controls>
					<OnMouseDown>
						self:TakeFocus()
					</OnMouseDown>
					<OnEnter>
						self:LoseFocus()
					</OnEnter>
					<OnEscape>
						self:LoseFocus()
					</OnEscape>
				    <OnTextChanged>
						SetLocker.SearchbarChange()
					</OnTextChanged>
				</EditBox>
                <Control name="$(parent)Headers">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)_Text" relativePoint="BOTTOMLEFT" offsetX="0" offsetY="5"/>
                    <Dimensions y="32" />
                    <Controls>
						<Control name="$(parent)Expand" inherits="ZO_SortHeader">
                            <OnInitialized>
                                ZO_SortHeader_Initialize(self, "", "expand", ZO_SORT_ORDER_DOWN, TEXT_ALIGN_LEFT, "ZoFontGameLargeBold")
                            </OnInitialized>
                            <Anchor point="TOPLEFT" />
                            <Dimensions x="26" y="32" />
                        </Control>
                        <Control name="$(parent)Set" inherits="ZO_SortHeader">
                            <OnInitialized>
                                ZO_SortHeader_Initialize(self, "Set Name", "name", ZO_SORT_ORDER_DOWN, TEXT_ALIGN_LEFT, "ZoFontGameLargeBold")
                            </OnInitialized>
                            <Anchor point="TOPLEFT" relativeTo="$(parent)Expand" relativePoint="TOPRIGHT" />
                            <Dimensions x="250" y="32" />
                        </Control>
                        <Control name="$(parent)Locked" inherits="ZO_SortHeader">
                            <OnInitialized>
                                ZO_SortHeader_Initialize(self, GetString(SI_SETLOCKER_LOCK), "status", ZO_SORT_ORDER_DOWN, TEXT_ALIGN_LEFT, "ZoFontGameLargeBold")
                            </OnInitialized>
                            <Anchor point="TOPLEFT" relativeTo="$(parent)Set" relativePoint="TOPRIGHT"/>
                            <Dimensions x="128" y="32" />
                        </Control>
                    </Controls>
                </Control>
                <Control name="$(parent)List" inherits="ZO_ScrollList">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)Headers" relativePoint="BOTTOMLEFT" />
                    <Anchor point="BOTTOMRIGHT" offsetX="-12" offsetY="-64" />
                </Control>			
            </Controls>
        </TopLevelControl>
        <Button name="ScrollListUnitRow" virtual="true">
            <Dimensions y="32" />
            <Anchor point="RIGHT" relativeTo="$(parent)" />
            <OnMouseEnter>
                SetLocker.MouseEnter(self)
            </OnMouseEnter>
            <OnMouseExit>
                SetLocker.MouseExit(self)
            </OnMouseExit>
            <OnMouseUp>
                SetLocker.MouseUp(self, button, upInside)
            </OnMouseUp>
            <Controls>
					<Button name="$(parent)Expand">
						<Dimensions x="26" y="26"/>
						<Anchor point="TOPLEFT" relativeTo="$(parent)"/>
						<Textures
							normal="EsoUI/Art/Buttons/plus_up.dds"
							pressed="esoui/art/buttons/minus_up.dds"
						/>
						<OnClicked>SetLocker.ExpandSet(self)</OnClicked>
					</Button>
					<Label name="$(parent)Set" font="ZoFontWinH4" wrapMode="ELLIPSIS">
						<Anchor point="TOPLEFT" relativeTo="$(parent)Expand" relativePoint="TOPRIGHT" />
						<Dimensions x="250" y="32" />
					</Label>
					<Label name="$(parent)Locked" font="ZoFontWinH4" wrapMode="ELLIPSIS">
						<Anchor point="TOPLEFT" relativeTo="$(parent)Set" relativePoint="TOPRIGHT" />
						<Dimensions x="128" y="32" />
					</Label>
            </Controls>
        </Button>
